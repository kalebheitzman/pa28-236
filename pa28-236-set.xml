<?xml version="1.0"?>
<PropertyList>
	<sim include="Systems/views.xml">
		
		<!-- Basic Details -->
		<description>Piper Dakota</description>
		<aircraft-version>0.0.1</aircraft-version>
		<author>Kaleb Heitzman</author>
		<flight-model>yasim</flight-model>
		<aero>pa28-236</aero>
		
		<!-- Status and Rating -->
		<status>development</status>
		<rating>
			<FDM type="int">2</FDM>
			<systems type="int">1</systems>
			<cockpit type="int">0</cockpit>
			<model type="int">2</model>
		</rating>
		
		<!-- Splash Details -->
		<startup>
			<splash-texture>Aircraft/pa28-236/Splashes/splash.png</splash-texture>
			<splash-title>Piper Dakota</splash-title>
		</startup>
		
		<!-- Model Details -->
		<model>
			<path>Aircraft/pa28-236/Models/pa28-236.xml</path>
		</model>
    <panel>
      <visibility>false</false>
    </panel>

    <!-- Systems -->
    <systems>
      <autopilot>
        <name>autopilot helpers</name>
        <path>Aircraft/Generic/generic-autopilot-helper.xml</path>
      </autopilot>
      <autopilot>
        <path>Aircraft/pa28-236/Systems/kap140.xml</path>
      </autopilot>
      <autopilot>
        <name>FiltersOnly</name>
        <path>Aircraft/c172p/Systems/NAVandGSfilters.xml</path>
      </autopilot>
      <electrical>
        <path></path>
      </electrical>
    </systems>
		
		<!-- Sound Details -->
		<sound>
			<path>Aircraft/pa28-236/pa28-236-sound.xml</path>
		</sound>
		
		<!-- Flaps -->
		<flaps>
    	<setting>0.00</setting>
    	<setting>0.5</setting> <!-- 15 deg -->
    	<setting>1.00</setting> <!-- 30 deg -->
  	</flaps>

		<!-- Menubar -->
		<menubar include="Dialogs/pa28-236-menu.xml" />

		<!-- Weights -->
		<weight n="0">
	    <name>Pilot</name>
	    <weight-lb>170</weight-lb>
	    <max-lb>300</max-lb>
	  </weight>
	  <weight n="1">
	    <name>Copilot</name>
	    <weight-lb>0</weight-lb>
	    <max-lb>300</max-lb>
	  </weight>
	  <weight n="2">
	    <name>Left Passenger</name>
	    <weight-lb>0</weight-lb>
	    <max-lb>300</max-lb>
	  </weight>
	  <weight n="3">
	    <name>Right Passenger</name>
	    <weight-lb>0</weight-lb>
	    <max-lb>300</max-lb>
	  </weight>
	  <weight n="4">
	    <name>Baggage</name>
	    <weight-lb>200</weight-lb>
	    <max-lb>200</max-lb>
	  </weight>
		
	</sim>

  <!-- Instruments -->
	<instrumentation>
		<kma20 include="Aircraft/Instruments-3d/kma20/kma20init.xml" />
	</instrumentation>

  <!-- Nasal Scripts -->
	<nasal>
	  <pa28>
	    <file>Aircraft/pa28-161/Nasal/pa28.nas</file>
	  </pa28>
	  <kap140>
	    <file>Aircraft/Generic/kap140.nas</file>
	  </kap140>
	</nasal>
	
</PropertyList>